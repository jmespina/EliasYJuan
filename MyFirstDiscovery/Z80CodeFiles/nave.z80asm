; Code file
start: .org #8000
ld			hl, udgsCommon
ld			(UDG), hl
ld		bc, (shipPos)
call	At
ld		a, $90
rst		$10

loop:
    jp loop

jp #12a2

At:
push	de	; Preserva el valor de DE
push	hl	; Preserva el valor de HL
call	$0a23	; Llama a la rutina de la ROM
pop		hl	; Recupera el valor de HL
pop		de	; Recupera el valor de DE

ret

udgsCommon:
db $24, $42, $99, $bd, $ff, $18, $24, $5a	; $90 Nave
shipPos: dw $0511
UDG:			EQU $5c7b

